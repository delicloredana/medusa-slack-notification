{
  "name": "medusa-plugin-slack-notification",
  "private": true,
  "type": "module",
  "workspaces": [
    "dev/medusa",
    "slack-notification"
  ],
  "scripts": {
    "start:medusa": "nodemon --watch \"slack-notification\" --ignore \"slack-notification/src\" --ignore \"slack-notification/thunder\" --spawn --exec \"npm start -w medusa\" --delay 2 --verbose",
    "start": "concurrently -n \"build:slack-notification\" \"npm run build -w medusa-plugin-slack-notification && npm run build -w medusa\"  && concurrently -n \"start:medusa,watch:slack-notification\" \"npm run start:medusa\" \"npm run watch -w medusa-plugin-slack-notification\"  --timings --prefix \"[{name}] [{time}] =>\""
  },
  "dependencies": {
    "@agilorunner/gitdeploy": "^2.4.0",
    "@agilorunner/run": "^2.4.0",
    "@medusajs/medusa-oas-cli": "file:.yalc/@medusajs/medusa-oas-cli",
    "concurrently": "^8.2.2",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "nodemon": "^3.0.1",
    "openapi-filter": "^3.2.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/fs-extra": "^11.0.4"
  }
}
